{
  "name": "@octocoach/db",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "generate": "drizzle-kit generate:pg",
    "generate:org": "drizzle-kit generate:pg --schema=\"./src/schema/organization/**/*.ts\" --out=\"org\"",
    "migrate": "tsx -r dotenv/config src/migrate.ts",
    "seed": "tsx -r dotenv/config src/seed.ts",
    "read": "tsx -r dotenv/config src/read.ts",
    "test": "vitest",
    "check": "tsx src/schema/organization/members.ts",
    "create:org": "tsx -r dotenv/config src/create-org.ts",
    "new:org": "tsx -r dotenv/config src/new-org.ts"
  },
  "dependencies": {
    "@auth/core": "^0.14.0",
    "@types/cli-progress": "^3.11.0",
    "cli-progress": "^3.12.0",
    "drizzle-orm": "^0.28.6",
    "drizzle-zod": "^0.4.4",
    "got": "^12.6.0",
    "langchain": "^0.0.145",
    "postgres": "^3.3.5",
    "superjson": "^1.13.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/node": "^20.3.2",
    "dotenv": "^16.0.3",
    "drizzle-kit": "^0.19.13",
    "tsconfig": "workspace:*",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6"
  }
}
