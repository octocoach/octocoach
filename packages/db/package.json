{
  "name": "@octocoach/db",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "generate": "drizzle-kit generate:pg",
    "generate:org": "drizzle-kit generate:pg --config=./org.drizzle.config.ts",
    "migrate": "tsx -r dotenv/config ./migrate.ts",
    "seed": "tsx -r dotenv/config seed.ts",
    "read": "tsx -r dotenv/config src/read.ts",
    "test": "vitest",
    "check": "tsx src/schema/organization/members.ts",
    "create:org": "tsx -r dotenv/config src/create-org.ts",
    "new:org": "tsx -r dotenv/config src/new-org.ts",
    "get:schema": "tsx -r dotenv/config src/get-schema-files.ts",
    "test:connection": "tsx -r dotenv/config ./test-connection.ts"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.6.0",
    "@octocoach/auth": "workspace:^",
    "@types/cli-progress": "^3.11.0",
    "@vercel/postgres": "^0.5.1",
    "cli-progress": "^3.12.0",
    "drizzle-orm": "^0.29.1",
    "drizzle-zod": "^0.5.1",
    "langchain": "^0.0.180",
    "minimist": "^1.2.8",
    "nanoid": "^4.0.2",
    "nanoid-dictionary": "^4.3.0",
    "octokit": "^3.1.0",
    "postgres": "^3.3.5",
    "superjson": "^1.13.1",
    "undici": "^5.27.2",
    "ws": "^8.15.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@octocoach/i18n": "workspace:^",
    "@types/minimist": "^1.2.2",
    "@types/nanoid-dictionary": "^4.2.3",
    "@types/node": "^20.3.2",
    "@types/ws": "^8.5.10",
    "dotenv": "^16.0.3",
    "drizzle-kit": "^0.20.6",
    "tsconfig": "workspace:*",
    "tsx": "^3.12.7",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "drizzle-kit": "^0.19.13"
  }
}
